[user]
	email = a.mamaevs@gmail.com
	name = Aleksey Mamaev
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = master
[core]
	quotepath = off
[alias]
  history = log --graph
  hst = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  